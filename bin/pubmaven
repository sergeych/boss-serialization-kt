#!/bin/zsh

function error() {
    echo "$1"
    exit 100
}
. <(zetext -d ~/universa_maven.ztext)
if [ $? -ne 0 ]; then error "zetext decryption failed"; fi

if [ -z "${maven_user}" ]; then error "maven user is not set"; fi
if [ -z "${maven_password}" ]; then error "maven passowrd is not set"; fi
./gradlew publish
